<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="ChatClient.App"
             RequestedThemeVariant="Dark">
    <Application.Styles>
		<FluentTheme>
			<FluentTheme.Palettes>
				<!-- Palette for Dark theme variant -->
				<ColorPaletteResources x:Key="Dark" Accent="Purple" RegionColor="#171B2D" ErrorText="Orange"/>
			</FluentTheme.Palettes>
		</FluentTheme>
    </Application.Styles>
</Application>
